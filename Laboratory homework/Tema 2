const int potPin  = A2;
const int ledPin  = 3;

int potValue = 0;
int ledValue = 0;

void setup() {
 pinMode(potPin,INPUT);
 pinMode(ledPin,OUTPUT);
 Serial.begin(9600);
}

void loop() {
  potValue = analogRead(potPin);
  Serial.println(potValue);
  ledValue = map(potValue , 0,1023 , 0,255);
  analogWrite(ledPin,ledValue);
  
}

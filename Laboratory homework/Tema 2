const int knockPin  = A2;
const int ledPin  = 3;

int knockValue = 0;
int ledValue = 0;

void setup() {
 pinMode(knockPin,INPUT);
 pinMode(ledPin,OUTPUT);
 Serial.begin(9600);
}

void loop() {
  knockValue = analogRead(knockPin);
  Serial.println(knockValue);
  ledValue = map(knockValue , 0,1023 , 0,255);
  analogWrite(ledPin,ledValue);
  
}
